enemys={}
enemyhp=0

function update_enemys(enemy) 
 if enemy.hp<=0 then
  enemyhp-=10
  player.score+=100
  new_enemy(enemy.espr,enemy.damagecount,enemy.spd)
  del(enemys,enemy)
 end
 
 local cx=enemy.erndx-enemy.x
  
	enemy.x+=cx*enemy.spd
	 
	if flr(enemy.x)==flr(enemy.erndx) then
	 bcolor=8
	 player.hp-=enemy.damagecount
	 enemy.erndx=rnd(80)
	end
end

function draw_enemys(enemy)
 spr(enemy.espr,enemy.x,enemy.y,2,3)
end

function new_enemy(_spr,dc,_spd)

 add(enemys,{ 
	 x=rnd(100),
	 y=80,
	 spd=_spd,
	 width=16,
	 height=32,
	 espr=_spr,
	 isdead=false,
		isright=false,
		hp=10,
		erndx=rnd(80),
		damagecount=dc
	})
	
end

function enemys_init()
 foreach(enemys,init_enemys)
end

function enemys_update()
 foreach(enemys,update_enemys)
end

function enemys_x_pos()
 for i in all(enemys) do
  return i.x
 end
end

function enemys_y_pos()
  for i in all(enemys) do
  return i.y
 end
end

function enemy_draw()
 foreach(enemys,draw_enemys)
 print("enemy core hp: "..enemyhp,28,20,8)
end
