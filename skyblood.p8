charlist={"white_manner","iron_khan","gorden_passer"}
curchar=nil
cut=1

menu_state=
{
 "select_char",
 "ready_countdown",
 "start_countdown"
}

cur_menu=menu_state[1]


function update_logo()
 if btnp(‚¨ÖÔ∏è) and cut>1 then
  cut-=1
 elseif btnp(‚û°Ô∏è) and cut<3 then
  cut+=1
 end
 
 if btnp(üÖæÔ∏è) then
  curchar=charlist[cut]
  
  if curchar==charlist[1] then
   new_enemy(2,3,rnd(0.05))
   enemyhp=300
  elseif curchar==charlist[2] then
   new_enemy(4,6,rnd(0.08))
   enemyhp=520
  elseif curchar==charlist[3] then
   new_enemy(6,9,rnd(0.12))
   enemyhp=580
  end
  
  cur_menu=menu_state[2]
  
 end
end

countdown_wait=3

function wait_countdown_update()
 music(-1)
 countdown_wait-=0.025
 
 if countdown_wait<0 then
  cur_menu=menu_state[3]
  countdown_wait=3
 end
end

function wait_countdown_draw()
 print("get ready player!!",27,64,12)
end

function start_countdown_update()
 countdown_wait-=0.025 
 
 if countdown_wait<0 then
  music(7)
  game_state="game_mode"
  countdown_wait=3
 end
end

function start_countdown_draw()
