game_state="menu"
bcolor=0
damage_time=0

function _init()
 bcolor=0
 damage_time=0
 music(0)
end

function _update60()
 if game_state=="menu" then
  if cur_menu==menu_state[1] then
   update_logo()
  elseif cur_menu==menu_state[2] then
   wait_countdown_update()
  elseif cur_menu==menu_state[3] then
   start_countdown_update()
  end
 elseif game_state=="game_mode" then
	 
	 if not player.isdead then
		 enemys_update()
		 player.update()
		 
		 if bcolor!=0 then
		  damage_time+=0.5
		  if damage_time>1 then
		   damage_time=0
		   bcolor=0
		  end
		 end
		else
		 game_state="game_over"
		end
		
		if enemyhp<=0 then
		 game_state="clear"
		end
		
	elseif game_state=="game_over" or 
	       game_state=="clear" then
	 replayfunc()
	end
end

function _draw()
 if game_state=="menu" then
  cls(1)
  if cur_menu==menu_state[1] then
   draw_logo() 
  elseif cur_menu==menu_state[2] then
   wait_countdown_draw()
  elseif cur_menu==menu_state[3] then
   start_countdown_draw()
  end
 elseif game_state=="game_mode" then
 	cls(bcolor)
 
	 line(0,120,30,100,5)
	 line(125,120,95,100,5)
	 line(60,125,60,100,5)
	 line(0,110,125,110,5)
	 line(0,100,125,100,5)
 	
 	enemy_draw()
 	player.draw()
 elseif game_state=="game_over" then
	 cls(8)
	 game_over_draw()
	elseif game_state=="clear" then
	 cls()
	 game_clear_draw()
	end
end
