-- skyblood : pico-8 prototype
-- player function list
-- skagogames 2022. made in skago.

player=
{
 pspr=1,
 x=rnd(120),
 y=rnd(120),
 width=8,
 height=8,
 spdx=0.1,
 spdy=0.1,
 isdead=false,
 isfire=false,
 radfire=8,
 hp=100,
 score=0,
 statetime=0,
 cooltime=false,
 ltime=100
}

player.update=function()
 player.ltime-=0.04
 
 if player.hp<=0 and not 
    player.isdead then
  player.isdead=true
 end
 
 if player.ltime<0 then
  player.isdead=true
 end
 
 -- player speed movement
 player.x+=player.spdx
	player.y+=player.spdy
	
	-- player speed limit
	if player.spdx<=-3 then
	 player.spdx=-3
	elseif player.spdx>=3 then
	 player.spdx=3
	end
	
	if player.spdy<=-3 then
	 player.spdy=-3
	elseif player.spdy>=3 then
	 player.spdy=3
	end
	
	-- player screen xy
	if player.x<-5 then
	 player.x=120
	elseif player.x>126 then
	 player.x=-5
	end
	
	if player.y<-5 then
	 player.y=120
	elseif player.y>126 then
	 player.y=-5
	end
	
	if player.x<-5 and player.y<-5 then
	 player.x=120
	 player.y=120
	elseif player.x>126 and player.y>126 then
	 player.x=-5
	 player.y=-5
	end
	
	-- player controller 
	if btn(‚¨ÖÔ∏è) then
	 player.spdx-=0.05
	elseif btn(‚û°Ô∏è) then
	 player.spdx+=0.05
	end
	 
	if btn(‚¨ÜÔ∏è) then
	 player.spdy-=0.05
	elseif btn(‚¨áÔ∏è) then
	 player.spdy+=0.05
	end
	 
	-- return center position
	
	if btn(‚¨ÖÔ∏è) or
			 btn(‚û°Ô∏è) or
			 btn(‚¨ÜÔ∏è) or
			 btn(‚¨áÔ∏è) then
	  player.statetime=0
	else
	 player.statetime+=0.05
	end
	
	if player.statetime>10 then
	 player.statetime=10
	 
	 local cx=60-player.x
	 local cy=60-player.y
	  
	 player.x+=cx*0.05
	 player.y+=cy*0.05
	  
	 player.spdx=0
	 player.spdy=0
	 
	 if btn(‚¨ÖÔ∏è) or
	    btn(‚û°Ô∏è) or
	    btn(‚¨ÜÔ∏è) or
	    btn(‚¨áÔ∏è) then
	  player.statetime=0
	 end
	
 end
	
	-- is player weapon fire?
 if player.isfire then
    sfx(0)
	 player.radfire-=3
	 	  
	 if player.radfire<0 then
	  player.radfire=8
	  player.isfire=false
	 end
 end
 
 -- press üÖæÔ∏è key fire!
 if btnp(üÖæÔ∏è) then
 
  for e in all(enemys) do
   player.hit(e)
  end
  
  player.isfire=true
 end
 
 if btn(‚ùé) then
   local cx=flr(enemys_x_pos())-flr(player.x)
		 local cy=flr(enemys_y_pos())-flr(player.y)
		  
		 player.x+=cx*0.05
		 player.y+=cy*0.05
		 
		 player.spdx=0
		 player.spdy=0
		 
		 player.cooltime=true
		 player.hp-=2
 else
  if player.cooltime and 
     player.hp<100 then
   player.hp+=1
  elseif player.hp==100 then
   player.cooltime=false
  end
 end
end

player.draw=function()
 -- print ui
 print("hp:"..player.hp,3,114,12)
 print("score:"..player.score,3,120,12)
 print("time:"..flr(player.ltime),98,120,12)
 -- if weapon fire false.. show player.
 if not player.isfire then
  rect(0,0,127,127,12)
  rect(1,1,126,126,12)
  
	 spr(player.pspr,player.x,player.y)
	 
	 -- line x 
	 line(-128,player.y+3,player.x-2,player.y+3,12)
	 line(-128,player.y+4,player.x-2,player.y+4,12)
	 
	 line(128,player.y+3,player.x+9,player.y+3,12)
	 line(128,player.y+4,player.x+9,player.y+4,12)
	 
	 -- line y
	 line(player.x+3,player.y-2,player.x+3,-128,12)
	 line(player.x+4,player.y-2,player.x+4,-128,12)
	 
	 line(player.x+3,player.y+9,player.x+3,128,12)
	 line(player.x+4,player.y+9,player.x+4,128,12)

 else -- else.. fire_effect showed!
  circ(player.x+4,player.y+3,player.radfire+3,12)
  circfill(player.x+4,player.y+3,player.radfire,12)
 end
end

player.takedamage=function(d)
 player.hp-=d
end

-- player hit check!
player.hit=function(t)
 if player.x+player.width>t.x and
    player.x<t.x+t.width and
    player.y+player.height>t.y and
    player.y<t.y+t.height then
    
    bcolor=1
    t.hp-=10
 end
end
